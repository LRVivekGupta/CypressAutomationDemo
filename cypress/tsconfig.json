{
  "compileOnSave": true,
  "buildOnSave": true,
  "compilerOptions": {
    "baseUrl": "./cypress/",
    "sourceMap": false,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "types": ["cypress", "node"],
    "lib": ["es6", "dom", "es2017"]
  }
}
